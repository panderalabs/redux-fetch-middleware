{
  "extends": "eslint-config-airbnb",
  "env": {
    "mocha": true,
    "node": true
  },
  "globals": {
    "expect": true,
    "fetch": true
  },
  "rules": {
    "padded-blocks": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-expressions": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}
